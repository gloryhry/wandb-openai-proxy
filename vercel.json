{
  "routes": [
    {
      "src": "/v1/models",
      "dest": "/api/v1/models"
    },
    {
      "src": "/v1/chat/completions",
      "dest": "/api/v1/chat/completions"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index"
    }
  ],
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ]
}